<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
                             http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>org.fao.ess</groupId>
    <artifactId>fenix-reports-dist</artifactId>
    <packaging>pom</packaging>
    <version>1.0</version>
    <name>Fenix report parent</name>

    <modules>
        <module>survey-export</module>
        <module>hq-dist</module>
    </modules>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <jackson.version>2.4.3</jackson.version>
        <guava.version>18.0</guava.version>
        <crossdomain.version>1.1.2</crossdomain.version>
        <itext.version>5.5.5</itext.version>
        <fenix-export.version>1.0-SNAPSHOT</fenix-export.version>
        <resteasy.version>3.0.6.Final</resteasy.version>
    </properties>


    <distributionManagement>
        <repository>
            <id>FAO projects</id>
            <name>FAO projects-releases</name>
            <url>http://10.5.67.135/artifactory/libs-release-local</url>
        </repository>
    </distributionManagement>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.7</source>
                    <target>1.7</target>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <skipTests>true</skipTests>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <dependencies>
        <!-- FENIX export core -->
        <dependency>
            <groupId>org.fao.fenix</groupId>
            <artifactId>fenix-export</artifactId>
            <version>${fenix-export.version}</version>
        </dependency>
    </dependencies>


</project>
